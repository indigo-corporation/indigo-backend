{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/kirill2012/imdbphp"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-gd": "*",
        "ext-curl": "*",
        "arcanedev/log-viewer": "^10.0",
        "astrotomic/laravel-translatable": "^11.12",
        "cviebrock/eloquent-sluggable": "^10.0",
        "elasticsearch/elasticsearch": "^8.8",
        "guzzlehttp/guzzle": "^7.7",
        "imdbphp/imdbphp": "dev-fix",
        "intervention/image": "^2.7",
        "jenssegers/date": "^4.0",
        "laravel/framework": "^10.16",
        "laravel/horizon": "^5.19",
        "laravel/sanctum": "^3.2",
        "laravel/socialite": "^5.8",
        "laravel/tinker": "^2.8",
        "nnjeim/world": "^1.1.27",
        "predis/predis": "^2.2",
        "spatie/laravel-backup": "^8.1",
        "spatie/laravel-image-optimizer": "^1.7",
        "spatie/laravel-medialibrary": "^10.11",
        "stevebauman/location": "^7.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.10",
        "laravel/sail": "^1.23",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^7.7",
        "phpunit/phpunit": "^10.2",
        "spatie/laravel-ignition": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-ide-helper"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
